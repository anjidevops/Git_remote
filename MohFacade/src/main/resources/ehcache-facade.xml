<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">

    <defaultCache eternal="true" maxElementsInMemory="99000" overflowToDisk="true"/>

    <cache name="slotCache" eternal="false" maxElementsInMemory="5000" overflowToDisk="true" diskPersistent="false"
           timeToIdleSeconds="0" memoryStoreEvictionPolicy="LRU"/>

</ehcache>